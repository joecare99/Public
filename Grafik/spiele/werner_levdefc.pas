var
  FBild: array of array of array of byte;

procedure GetLevDef(x, y, lev: integer; Out Result: byte);

begin
  Result := FBild[lev, x, y];
end;

procedure SetLevDef(x, y, lev: integer; lTile: byte);
begin
  if lev > High(FBild) then
    SetLength(FBild, lev + 1, 20, 12);
  FBild[lev, x, y] := lTile;
end;

var
  lev, x, y: integer;

initialization

  SetLength(FBild, High(obild) + 1, 20, 12);
  for lev := 1 to High(obild) do
    for y := 0 to 11 do
      for x := 0 to 19 do
        FBild[lev, x, y] := (obild[lev, y] shr ((19 - x) * 3)) and 7;

finalization

  SetLength(FBild, 0, 0, 0);

end.
